{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@lib/*": [
        "./lib/*"
      ],
      "@context/*": [
        "./contexts/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
    },
    "typeRoots": [
      "./types"
    ]
  },
  "extends": "expo/tsconfig.base"
}